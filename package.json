{
	"name": "helloworld",
	"displayName": "HelloWorld",
	"description": "it is a test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:helloworld.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "helloworld.helloWorld",
				"title": "Hello"
			}
		],
		"keybindings":{
			"command": "helloworld.helloWorld",
			"key": "ctrl+u",
			"mac": "ctrl+u"
		},
		"menus": {
			"editor/context": [
					{
							"when": "editorFocus",
							"command": "helloworld.helloWorld",
							"group": "navigation"
					}
			],
			"editor/title": [
					{
							"when": "editorFocus",
							"command": "helloworld.helloWorld",
							"group": "navigation"
					}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "beautifulGirl",
					"title": "美女",
					"icon": "images/beautifulGirl.svg"
				}
			]
		},
		"views": {
			"beautifulGirl": [
				{
					"id": "beautifulGirl1",
					"name": "国内美女"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
